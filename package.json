{
  "name": "ionic8-playground",
  "description": "A playground application for Ionic 8 with React.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mwarman/ionic8-playground"
  },
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
